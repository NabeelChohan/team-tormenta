<p id="notice"><%= notice %></p>

<h1>Listing Weather Updates</h1>

<table>
  <thead>
    <tr>
      <th>Zip</th>
      <th>Date</th>
      <th>Time1</th>
      <th>Time2</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @weather_updates.each do |weather_update| %>
      <tr>
        <td><%= weather_update.zip %></td>
        <td><%= weather_update.date %></td>
        <td><%= weather_update.time1 %></td>
        <td><%= weather_update.time2 %></td>
        <td><%= link_to 'Show', weather_update %></td>
        <td><%= link_to 'Edit', edit_weather_update_path(weather_update) %></td>
        <td><%= link_to 'Destroy', weather_update, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Weather update', new_weather_update_path %>
